<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.chao.myapplication.MainActivity">

    <Button
        android:id="@+id/test_user"
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:onClick="startTestUserActivity"
        android:text="TestUSer"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_editor_absoluteY="234dp"
        tools:text="TestUSer" />

    <Button
        android:id="@+id/primary_user"
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:layout_marginTop="96dp"
        android:onClick="startPrimaryUserActivity"
        android:text="PrimaryUser"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</android.support.constraint.ConstraintLayout>
